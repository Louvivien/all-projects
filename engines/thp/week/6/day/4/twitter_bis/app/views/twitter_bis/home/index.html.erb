<h1>New Tweet</h1>

<%= link_to 'Back', root_path %>


<%= form_for :tweet, :url =>{:action => "index", :id =>:tweet} do |f| %>
    <p>Title: <%= f.text_field 'title' %></p>
    <p>content: <%= f.text_field  'content' %></p>
    <p>User[id|name]: <%= f.text_field  'user' %></p>
    <p>Argument suppltr: <%= f.text_field  'arg_config' %></p>
    <p>Description<br/>
    <%= f.text_area 'description' %></p>
    <%= f.submit "Send Tweet" %>
<% end %>

<%= link_to 'Back', {:action => 'index' } %>
